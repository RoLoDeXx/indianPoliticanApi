  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-6">
        <h1 class="h1-responsive">Bio</h1>
        <div class="card">
          <img class="card-img-top" src="img/rahul.webp" alt="Card image cap">
          <div class="card-body">
            <h4 class="card-title">{{politcian.name}}</h4>
